{
  "name": "server",
  "version": "0.0.1",
  "description": "node.js application",
  "main": "server.js",
  "scripts": {
    "_knex": "node_modules/.bin/knex",
    "start": "npm run dev",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./server.ts",
    "init": "npm run migrate && npm run seed",
    "lint": "tslint --project ./tsconfig.json",
    "migrate": "npm run _knex -- migrate:latest",
    "rollback": "npm run _knex -- migrate:rollback",
    "seed": "npm run _knex -- seed:run",
    "migrate:make": "npm run _knex -- migrate:make",
    "seed:make": "npm run _knex -- seed:make"
  },
  "engines": {
    "node": "4.2.x",
    "npm": "3.3.x"
  },
  "author": "Asen Yovchev",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.14.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "pg": "^7.3.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.37",
    "@types/knex": "0.0.37",
    "@types/node": "^8.0.28",
    "@types/uuid": "^3.4.2",
    "eslint": "^4.6.1",
    "eslint-plugin-classes": "^0.1.1",
    "nodemon": "^1.12.0",
    "ts-node": "^3.3.0",
    "typescript": "2.4.2"
  }
}
